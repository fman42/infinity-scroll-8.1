{
  "name": "monstrikus/infinity_scroll_php",
  "description": "Large Object Transmission Control Protocol",
  "keywords": [],
  "homepage": "https://github.com/monstrikus/infinity_scroll_php",
  "version": "0.0.1",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "laravel/framework": "*",
    "illuminate/database": "*",
    "illuminate/http": "*",
    "illuminate/validation": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "ext-pdo": "*"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "autoload": {
    "psr-4": {
      "InfinityScrollPagination\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "support": {
    "issues": "https://github.com/facade/ignition/issues",
    "source": "https://github.com/facade/ignition"
  }
}
